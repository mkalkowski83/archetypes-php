{
  "require": {
    "php": "8.2",
    "ext-bcmath": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.2",
    "symfony/console": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/flex": "^2.4",
    "symfony/framework-bundle": "^7.1",
    "symfony/yaml": "^7.1",
    "webmozart/assert": "^1.11"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "bin-dir": "bin",
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": { "Party\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Party\\Test\\": "tests/" }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/var-dumper": "^7.0"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
